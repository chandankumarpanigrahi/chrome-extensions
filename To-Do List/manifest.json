{
    "manifest_version": 3,
    "name": "ToDo Extension",
    "version": "1.0",
    "description": "A simple To-Do list extension",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
        }
    },
    "permissions": ["storage"],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "web_accessible_resources": [
        {
        "resources": ["popup.html", "popup.js", "style.css"],
        "matches": ["<all_urls>"]
        }
    ]
}